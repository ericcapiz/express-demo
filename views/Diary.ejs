<%- include('./layout/Header') %>
<%- include('./layout/Navbar') %>

<div class="add-container">
    <div class="add">
        <p>Add to Diary</p>
        <a href="/add">Add</a>
    </div>
    <div class="diary">
        <% data.forEach((diary)=>{%>
            <div class="diary-container">
                <h3>Title: <%= diary.title %></h3>
                <p>Date: <%= diary.date %></p>
                <br />
                <p>Entry: <%= diary.description %></p>
                <!-- <a href="">More</a> -->
            </div>
    
        <%})  %> 
    </div>
</div>


<%- include('./layout/Footer') %>